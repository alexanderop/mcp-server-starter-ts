{
  "entryPointStrategy": "expand",
  "entryPoints": ["src"],
  "tsconfig": "tsconfig.json",
  "out": "docs",
  "exclude": ["**/*.test.ts", "tests/**/*", "build/**/*", "dev.js"],
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeInternal": true,
  "readme": "README.md",
  "navigationLinks": {
    "GitHub": "https://github.com/alexanderop/mcp-server-starter-ts",
    "MCP Protocol": "https://modelcontextprotocol.io"
  },
  "categorizeByGroup": true,
  "categoryOrder": ["Core", "Tools", "Resources", "Prompts", "Registry", "*"],
  "groupOrder": ["Variables", "Functions", "Classes", "Interfaces", "Type Aliases", "*"],
  "theme": "default",
  "githubPages": true,
  "hideGenerator": false,
  "searchInComments": true
}